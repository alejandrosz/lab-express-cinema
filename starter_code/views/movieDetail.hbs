<h1>Ironhack Cinema</h1>
<p>Your Movie</p>
{{#each movieFound}}
<div class="movie">
  <h2> {{this.title}}</h2>
  <img src={{this.image}} alt={{this.title}} />
  <h3>{{this.director}}</h3>
  <p>{{this.stars}}</p>
  <p>{{this.description}}</p>
  <p>{{this.showtimes}}</p>
  {{!-- <a href="/movies/{{this.id}}">View more</a> --}}
</div>

{{/each}}